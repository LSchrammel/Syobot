SYSTEM [ { "question": "Quais parâmetros estão incluídos no submenu Central de Leads?", "answer": "O submenu Central de Leads inclui os seguintes parâmetros: USA_CENTRAL_DE_LEADS, USA_CENTRAL_DE_LEADS_COM_MIDIA, USA_CENTRAL_DE_LEADS_COM_ORIGEM e CENTRAL_LEADS_GLOBAL." }, { "question": "Qual é a finalidade do parâmetro USA_CENTRAL_DE_LEADS?", "answer": "O parâmetro USA_CENTRAL_DE_LEADS habilita ou desabilita a configuração do módulo de Central de Leads." }, { "question": "O que acontece quando o parâmetro USA_CENTRAL_DE_LEADS é habilitado?", "answer": "Quando o parâmetro USA_CENTRAL_DE_LEADS é habilitado, ele libera a permissão do menu Central de Leads para os cadastros de usuário e grupos de usuários, além de habilitar a aba da Central de Leads dentro do cadastro de equipes." }, { "question": "Como é o comportamento do parâmetro USA_CENTRAL_DE_LEADS para uma concessionária que nunca teve o menu Central de Leads?", "answer": "Se a concessionária nunca teve o insert do menu Central de Leads, o parâmetro USA_CENTRAL_DE_LEADS virá com o valor NAO." }, { "question": "Como é o comportamento do parâmetro USA_CENTRAL_DE_LEADS para uma concessionária que já teve o insert do menu Central de Leads?", "answer": "Se a concessionária já teve o insert do menu Central de Leads, o parâmetro USA_CENTRAL_DE_LEADS virá com o valor SIM." }, { "question": "Quem tem permissão de acesso aos parâmetros da Central de Leads?", "answer": "Somente o usuário Syonet tem permissão de acesso aos parâmetros da Central de Leads." }, { "question": "Qual é a finalidade do parâmetro USA_CENTRAL_DE_LEADS_COM_MIDIA?", "answer": "O parâmetro USA_CENTRAL_DE_LEADS_COM_MIDIA determina se a informação de mídia será exibida no lead da Central de Leads." }, { "question": "Qual valor vem por padrão no parâmetro USA_CENTRAL_DE_LEADS_COM_MIDIA?", "answer": "Por padrão, o valor do parâmetro USA_CENTRAL_DE_LEADS_COM_MIDIA vem como SIM." }, { "question": "Qual é a finalidade do parâmetro USA_CENTRAL_DE_LEADS_COM_ORIGEM?", "answer": "O parâmetro USA_CENTRAL_DE_LEADS_COM_ORIGEM determina se a informação de origem será exibida no lead da Central de Leads." }, { "question": "Qual valor vem por padrão no parâmetro USA_CENTRAL_DE_LEADS_COM_ORIGEM?", "answer": "Por padrão, o valor do parâmetro USA_CENTRAL_DE_LEADS_COM_ORIGEM vem como SIM." }, { "question": "Qual é a finalidade do parâmetro CENTRAL_LEADS_GLOBAL?", "answer": "O parâmetro CENTRAL_LEADS_GLOBAL configura se o usuário participa de uma central de mais de uma loja e se ele pode pegar um lead apenas ou um lead por loja." }, { "question": "Qual valor vem por padrão no parâmetro CENTRAL_LEADS_GLOBAL?", "answer": "Por padrão, o valor do parâmetro CENTRAL_LEADS_GLOBAL vem como NAO." }, { "question": "O que deve ser feito após a implementação do módulo de Central de Leads?", "answer": "Após a implementação do módulo de Central de Leads, deve-se criar um ou mais usuários responsáveis por receber os leads e gerenciar sua distribuição para a equipe." }, { "question": "Qual é o procedimento para criar um usuário responsável pela Central de Leads?", "answer": "O procedimento é acessar o cadastro do usuário e dar-lhe acesso à permissão de menu Central de Leads, além de configurar a equipe responsável." }, { "question": "O que é necessário para garantir que o timer da Central de Leads funcione corretamente?", "answer": "É necessário manter o timer ativo para verificar os leads 'pegos' pelos usuários e não trabalhados durante o período estipulado." }, { "question": "O que acontece se um lead não for trabalhado dentro do tempo estipulado no timer da Central de Leads?", "answer": "Se o lead não for trabalhado dentro do tempo estipulado, ele voltará para o usuário gerenciador dos leads para ser pego e trabalhado por outro usuário da equipe." }, { "question": "Onde deve-se criar uma nova Área para configurar a Central de Leads?", "answer": "A nova Área para configurar a Central de Leads deve ser criada no menu Tabelas, podendo ser uma área já existente no CRM." }, { "question": "Quais informações são necessárias ao criar uma nova equipe na Central de Leads?", "answer": "Ao criar uma nova equipe na Central de Leads, deve-se informar um nome para a equipe, selecionar um Supervisor e escolher os agentes que farão parte da equipe." }, { "question": "O que é necessário fazer na aba Central de Leads ao configurar uma equipe?", "answer": "Na aba Central de Leads, é necessário marcar o checkbox 'Habilitar Central de Leads?' e selecionar o usuário detentor dos eventos." }, { "question": "O que deve ser configurado no campo 'Tempo para dar o primeiro andamento no evento'?", "answer": "No campo 'Tempo para dar o primeiro andamento no evento', deve-se informar o número de minutos que o usuário tem para trabalhar o lead após pegá-lo." }, { "question": "O que acontece se o tempo para dar o primeiro andamento no evento expirar?", "answer": "Se o tempo para dar o primeiro andamento no evento expirar, o evento voltará para o usuário responsável pela Central de Leads na próxima execução do timer." }, { "question": "Como é configurado o grupo de evento / tipo evento na Central de Leads?", "answer": "No grupo de evento / tipo evento, deve-se definir o usuário que deverá receber os leads, podendo criar um tipo de evento novo ou editar um já existente." }, { "question": "Quem deve ser selecionado no campo 'Usuário Detentor da Central de Leads'?", "answer": "No campo 'Usuário Detentor da Central de Leads', deve ser selecionado o usuário criado previamente para ser o responsável pela gestão dos leads." } ]